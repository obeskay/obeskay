{
  "name": "sticky-covers-payload",
  "description": "Este es el sticky back.",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "src/web"
  ],
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development PAYLOAD_CONFIG_PATH=src/payload.config.ts tsx watch src/server.ts",
    "dev:web": "npm --prefix src/web run dev -- -p 3000",
    "dev": "concurrently --kill-others --success first \"npm run dev:server\" \"npm run dev:web\"",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm run copyfiles && npm run build:payload && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.33.0",
    "@payloadcms/email-nodemailer": "^3.33.0",
    "@payloadcms/email-resend": "^3.33.0",
    "@payloadcms/next": "^3.33.0",
    "@payloadcms/richtext-lexical": "^3.33.0",
    "@payloadcms/richtext-slate": "^3.33.0",
    "@sendinblue/client": "^3.3.1",
    "@swc/core": "^1.4.8",
    "@types/express": "^4.17.9",
    "axios": "^1.4.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "graphql": "^16.10.0",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.0.2",
    "payload": "^3.33.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "concurrently": "^9.1.2",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3"
  }
}
